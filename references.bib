@Book{Zeigler1976,
  title = {Theory Of Modelling And Simulation.},
  label = {Zeigler1976},
  type = {book},
  author = {{Zeigler} and {B.P.}},
  year = {1976},
  url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-17444436824&partnerID=40&md5=1e2e58dfc1f89b29a323d0f2b4fc1d4b},
  abstract = {This book is divided into three parts with Part 1 introducing the basic concepts. Part 2 presents the systems theory concepts necessary for an understanding of Part 3, which sets forth the basic framework and discusses a number of issues and problems in modelling and simulation. Chapters 1 and 2 deal with model description and basic systems concepts. With the conceptual basis so developed, the author considers the basic elements of discourse--real system, experimental frame, base model, lumped model, and the computer--and the concepts of simplification, validation, and simulation, which involve relations among the basic elements. Chapters 3 through 7 deal with the dynamics of simulation, that is, with the generation of the temporal behaviour of a model. The concept of state and the formal description of models are developed in parallel with the description of how the computer carries out the simulation process. The last chapter of Part 1 (Chapter 8) returns to the subject of modelling. The author discusses an example of discrete event modelling and introduces the concept of homomorphism as a criterion for valid simplification. Part 2 consists of two chapters. Chapter 9 develops a hierarchy of system specifications ranging from the lowest behavioural level to the highest structural level. Chapter 10 develops a parallel hierarchy of preservation relations that interrelate specifications. Part 3 opens with an axiomization of the basic framework, using the systems concepts. A number of problems common to many domains are isolated and formulated within the framework. (from preface)},
  language = {English},
  source = {Scopus},
