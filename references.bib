@Article{Guo2020,
  title = {A Data-Driven Multi-Fidelity Simulation Optimization For Medical Staff Configuration At An Emergency Department In Hong Kong},
  doi = {10.1007/s10696-020-09395-3},
  label = {Guo2020},
