@Conference{Borovska2020400,
  title = {Models Of Production Systems Taking Into Account Uncertainties And Disturbances},
  doi = {10.1109/CSIT49958.2020.9321966},
  label = {Borovska2020400},
  type = {conference},
  author = {{Borovska} and {T.} and {Hryshyn} and {D.} and {Severilov} and {V.} and {Kolesnyk} and {I.} and {Shestakevych} and {T.}},
  journal = {2020 IEEE 15th International Scientific and Technical Conference on Computer Sciences and Information Technologies, CSIT 2020 - Proceedings},
  year = {2020},
  volume = {1},
  url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85100445833&doi=10.1109%2fCSIT49958.2020.9321966&partnerID=40&md5=994676969c8c2cf4f8033ad95b652b2b},
  abstract = {The study is devoted to the development and research of new models and methods for the analysis and synthesis of the management of modern integrated facilities - production, retail, recycling, which are in active stochastic environment of suppliers, competitors, consumers. The analysis of analogues and prototypes revealed factors that guarantee the failure of new research and developments: - based only on Gaussian statistics, simplified models of object dynamics, search methods of large systems optimization. The classification of sources and generating mechanisms of the disturbances is carried out, models of nonlinear transformations of stochastic processes are presented. The paper presents the formulation and solution of problems of analyzing the influence of uncertainties and disturbances based on: - building of the resource simulation models of objects - virtual reality statistics generators; - using the optimal aggregation methodology to calculate the optimal operational and strategic management in the simulation model. The problems of obtaining probability distributions for non-linear transformations, in particular for the cost, output functions, are set and solved. Examples of the analysis of hyperbolic rank distributions and multimodal income probability distributions for the producer systems of a certain production segment are given. The development results made it possible to substantiate the conclusion about essentially non-Gaussian statistics of modern production systems and the recommendation about the need to study the statistics of optimal adaptive developing systems.  2020 IEEE.},
