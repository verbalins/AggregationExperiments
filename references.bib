@Article{Brooks19961,
  title = {Choosing The Best Model: Level Of Detail, Complexity, And Model Performance},
  doi = {10.1016/0895-7177(96)00103-3},
  label = {Brooks19961},
  type = {article},
  author = {{Brooks} and {R.J.} and {Tobias} and {A.M.}},
