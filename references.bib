@Article{Maier2017,
  title = {Model Granularity In Engineering Design - Concepts And Framework},
  doi = {10.1017/dsj.2016.16},
  label = {Maier2017},
  type = {article},
  author = {{Maier} and {J.F.} and {Eckert} and {C.M.} and John Clarkson and {P.}},
