@article{ ISI:000478601500023,
Author = {Bonfiglio, Luca and Royset, Johannes O.},
Title = {{Multidisciplinary Risk-Adaptive Set-Based Design of Supercavitating
   Hydrofoils}},
Journal = {{AIAA JOURNAL}},
Year = {{2019}},
Volume = {{57}},
Number = {{8}},
