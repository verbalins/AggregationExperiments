@article{ ISI:000514833200001,
Author = {Shahhosseini, Mohsen and Martinez-Feria, Rafael A. and Hui, Guiping and
   Archontoulis, Sotirios V.},
Title = {{Maize yield and nitrate loss prediction with machine learning algorithms}},
