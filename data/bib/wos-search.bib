@article{ ISI:000510903800021,
Author = {Ai, Weiwei and Patel, Nitish D. and Roop, Partha S. and Malik, Avinash
   and Trew, Mark L.},
Title = {{Cardiac Electrical Modeling for Closed-Loop Validation of Implantable
   Devices}},
