@article{ ISI:000603669200009,
Author = {Donatelli, Carmine and Kalra, Tarandeep Singh and Fagherazzi, Sergio and
   Zhang, Xiaohe and Leonardi, Nicoletta},
Title = {{Dynamics of Marsh-Derived Sediments in Lagoon-Type Estuaries}},
Journal = {{JOURNAL OF GEOPHYSICAL RESEARCH-EARTH SURFACE}},
