@CONFERENCE{Arthur201920,
author={Arthur, C. and Didier, C. and Romain, B. and Abraham, E.-G. and Gaëtan, L.},
title={Comparison of meta-modelling approaches to account for tiller growth and development simulated by the L-grass functional-structural plant model},
journal={Proceedings - 2018 6th International Symposium on Plant Growth Modeling, Simulation, Visualization and Applications, PMA 2018},
year={2019},
pages={20-24},
