@ARTICLE{Siverd201828,
author={Siverd, C.G. and Hagen, S.C. and Bilskie, M.V. and Braud, D.H. and Peele, R.H. and Twilley, R.R.},
title={Hydrodynamic storm surge model simplification via application of land to water isopleths in coastal Louisiana},
journal={Coastal Engineering},
year={2018},
