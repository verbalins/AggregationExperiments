@CONFERENCE{Li2018304,
author={Li, J. and Jia, Y. and Xu, B. and Chen, F. and Yang, Z. and Li, X.},
title={An improvement scheme for the overall line effectiveness of a production line: A case study},
journal={2018 5th International Conference on Industrial Engineering and Applications, ICIEA 2018},
year={2018},
pages={304-309},
doi={10.1109/IEA.2018.8387115},
note={cited By 1},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85050163163&doi=10.1109%2fIEA.2018.8387115&partnerID=40&md5=552afdd35577209181c3bb4c08bcdbe7},
affiliation={School of Mechanical Science and Engineering, Jilin University, Changchun, China; Production Department of Engine Factory, Brilliance Auto, Shenyang, China},
abstract={This paper formulates an overall line effectiveness (OLE) improvement scheme for a production line on the basis of Fourier amplitude sensitivity test (FAST) and stochastic Kriging (SK) surrogate model. It is known that improving the key parameters that have great influence on the target performance can bring better effect on the performance than improving other parameters. Thus this paper is concentrated on improving the OLE of a production line through improving multiple key parameters related to the OLE. In the proposed procedure, FAST is used to identify the key parameters for the OLE and a SK surrogate model is built to evaluate the OLE of the production line instead of a simulation model, improving the work efficiency effectively. At last, a comparison with the continuous improvement process (CIP) shows that the scheme formulated with FAST and SK surrogate model is easier to be implemented than the one generated by CIP, because the procedure provided in this paper can give further suggestion to improve the OLE when the improvement of the bottleneck is limited. © 2018 IEEE.},
