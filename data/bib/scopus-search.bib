@ARTICLE{Jiang20183195,
author={Jiang, X. and Lu, W. and Na, J. and Hou, Z. and Wang, Y. and Chi, B.},
title={A stochastic optimization model based on adaptive feedback correction process and surrogate model uncertainty for DNAPL-contaminated groundwater remediation design},
journal={Stochastic Environmental Research and Risk Assessment},
year={2018},
