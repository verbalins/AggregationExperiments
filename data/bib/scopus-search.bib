@ARTICLE{Esteva2019,
author={Esteva, M. and Jansen, C. and Arduino, P. and Sharifi-Mood, M. and Dawson, C.N. and Balandrano-Coronel, J.},
title={Curation and publication of simulation data in Designsafe, a natural hazards engineering open platform and repository},
