@CONFERENCE{MarkEhsani2018,
author={Mark Ehsani, M. and Bashaireh, A.},
title={New Dynamic End-to-End Modeling of More Electric Aircraft Power Systems},
journal={2018 AIAA/IEEE Electric Aircraft Technologies Symposium, EATS 2018},
year={2018},
doi={10.2514/6.2018-5011},
art_number={8552787},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85059980645&doi=10.2514%2f6.2018-5011&partnerID=40&md5=ddfa4a4bc3001bdbdf181b9b919d300c},
affiliation={Texas AandM University, College Station, TX  77843, United States; Department of Electrical and Computer Engineering, United States},
abstract={Aircraft power and propulsion system design is evolving towards more electric technology for the purpose of improving overall efficiency. This poses new challenges for modelling, simulation, optimal design of systems containing mechanical and electrical components, including many switching power electronic converters. The generalized gyrator theory and time-domain transfer function theory provide new tools for studying the dynamics and stability of such electromechanical systems. This paper presents these concepts to help with the analysis, design and control of the more electric aircraft. Using the generalized gyrator theory, the mechanical components can be transferred to the electrical side of the system and representedby their electrical models. The time-domain transfer function theory of power electronics provides a method to model switching power electronics converters as a generalized transformer. This allows for the referral of circuit elements from one side of the switching converter to the other. As a result, the complex aircraft electromechanical system can be reduced to an electrical circuit simplified model. This will assist in the detailed analysis, design and control of the system, prior to physical prototyping (iron-bird). In addition, the overall system circuit model will provide a better insight into the operation and interactions between the components of the system. Basic theory, example case studies, and future applications and benefits to the more electric aircraft will be presented and discussed. © 2018 AIAA.},
keywords={Aircraft;  Electric vehicles;  Gyrators;  Power converters;  Power electronics;  Propulsion;  Time domain analysis;  Transfer functions, Electromechanical systems;  Future applications;  Mechanical and electrical;  Mechanical components;  More electric aircraft;  Overall efficiency;  Physical prototyping;  Switching converter, Aircraft power systems},
