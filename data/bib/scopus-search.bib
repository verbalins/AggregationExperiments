@ARTICLE{Cook2019,
author={Cook, J.A. and Smith, R.C. and Hite, J.M. and Stefanescu, R. and Mattingly, J.},
title={Application and evaluation of surrogate models for radiation source search},
journal={Algorithms},
year={2019},
volume={12},
number={2},
doi={10.3390/A12120269},
art_number={269},
note={cited By 2},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85079643959&doi=10.3390%2fA12120269&partnerID=40&md5=47a82c875dc358d39b30f240f2c12aa6},
affiliation={Department of Mathematics, North Carolina State University, Raleigh, NC  27695-8205, United States; Department of Nuclear Engineering, North Carolina State University, Raleigh, NC  27695-7909, United States; Global Validation Model Department, Spire Global, Boulder, CO  80301, United States},
abstract={Surrogate models are increasingly required for applications in which first-principles simulation models are prohibitively expensive to employ for uncertainty analysis, design, or control. They can also be used to approximate models whose discontinuous derivatives preclude the use of gradient-based optimization or data assimilation algorithms. We consider the problem of inferring the 2D location and intensity of a radiation source in an urban environment using a ray-tracing model based on Boltzmann transport theory. Whereas the code implementing this model is relatively efficient, extension to 3D Monte Carlo transport simulations precludes subsequent Bayesian inference to infer source locations, which typically requires thousands to millions of simulations. Additionally, the resulting likelihood exhibits discontinuous derivatives due to the presence of buildings. To address these issues, we discuss the construction of surrogate models for optimization, Bayesian inference, and uncertainty propagation. Specifically, we consider surrogate models based on Legendre polynomials, multivariate adaptive regression splines, radial basis functions, Gaussian processes, and neural networks. We detail strategies for computing training points and discuss the merits and deficits of each method. © 2019 by the authors.},
author_keywords={Bayesian inference;  Radiation source localization;  Surrogate modeling},
keywords={Backpropagation;  Bayesian networks;  Computation theory;  Inference engines;  Monte Carlo methods;  Polynomial regression;  Radial basis function networks;  Radiation;  Statistical mechanics;  Urban transportation, Bayesian inference;  Boltzmann transport theory;  Data assimilation algorithms;  First-principles simulations;  Gradient-based optimization;  Multivariate adaptive regression splines;  Radiation source;  Surrogate model, Uncertainty analysis},
