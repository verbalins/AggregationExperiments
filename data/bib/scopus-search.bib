@CONFERENCE{Wang20181,
author={Wang, M.-H. and Hu, I. and Chen, R.Y.C. and Yeh, C.-L. and Shih, M.-K. and Tarng, D.},
title={Strip warpage assessment of dual side molding SiP module},
journal={2017 IEEE 19th Electronics Packaging Technology Conference, EPTC 2017},
year={2018},
volume={2018-February},
pages={1-5},
doi={10.1109/EPTC.2017.8277508},
note={cited By 1},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85050673426&doi=10.1109%2fEPTC.2017.8277508&partnerID=40&md5=810fe16b009361cf684e3ae022517df1},
affiliation={Advanced Semiconductor Engineering Inc., Nantze Export Processing Zone, 26 Chin 3 Rd., Kaohsiung, 811, Taiwan},
abstract={SiP modules, integrates multi-function components in a package, are widely applied in consumer electronics and IoT, such as Wifi module, GPS module and RF module, etc. Dual side molding module is a more advanced device type, who integrate all the components on the both surfaces of the substrate to effectively reduce the module area. By process strip warpage control is one of the crucial matters for assembly in order to have good yield and high automatic manufacture efficiency. Finite element method is a powerful tool for strip warpage evaluation; however, dual side SiP module strip requires enormous elements to describe its details. A useful model simplification method was proposed in this study. The several layers substrate was simplified as a homogeneous material by numerical tensile and thermal expansion tests to obtain the substrate bulk material properties of modulus and thermal expansion coefficient. The simplified substrate not only helps for efficiency computing, but also can be a transition zone for the different component layout of top and bottom substrate surfaces successfully mesh in the simulation model. Birth and death method was used to investigate by process strip warpage, the evaluation results help to reduce the risk comes from strip warpage. According to the well validated simulation model, for process improvement, firstly SMT and molding top side has better performance for strip warpage; for EMC improvement, bottom side choose high shrinkage EMC or top side select low shrinkage EMC can well control strip warpage. © 2017 IEEE.},
keywords={Chip scale packages;  Efficiency;  Electromagnetic compatibility;  Molding;  Process control;  Shrinkage;  Silicon compounds;  System-in-package;  Thermal expansion, Evaluation results;  Homogeneous materials;  Model simplification;  Process Improvement;  Simulation model;  Substrate surface;  Thermal expansion coefficients;  Transition zones, Substrates},
