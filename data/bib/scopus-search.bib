@ARTICLE{Wang2020,
author={Wang, D. and Zheng, Z. and Ma, Q. and Hu, P. and Huang, X. and Song, Y.},
title={Effects of the errors between core heterogeneity and the simplified models on numerical modeling of CO2/water core flooding},
journal={International Journal of Heat and Mass Transfer},
