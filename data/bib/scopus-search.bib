@CONFERENCE{Fisher202035,
author={Fisher, A. and Adhikari, B. and Zhai, C. and Morgan, J.E. and Mago, V.K. and Giabbanelli, P.J.},
title={Predicting the resource needs and outcomes of computationally intensive biological simulations},
journal={Simulation Series},
year={2020},
volume={52},
number={1},
pages={35-46},
note={cited By 1},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85086825763&partnerID=40&md5=032a9cfdbfa3d89facfe194a5abec401},
affiliation={Dept. of Computer Science, Lakehead University, Thunder Bay, ON, Canada; Dept. of Computer Science and Software Engineering, Miami University, Oxford, OH, United States},
abstract={Accurately simulating viral dynamics within a human body is often computationally intensive, thus requiring dedicated computing infrastructures. This limits the use of simulations in personalized medicine since patients may not all have access to such infrastructures or the possibility of waiting weeks for predictions. Lowering the computational burden by simplifying the models is a challenge since simulation results must remain accurate in order to safely support the decision-making activities of patients. In this work, we use machine learning to predict the results of a simulation (i.e. surrogate model) and its computational resources (i.e. performance model). This allows patients to use machine learning as a computationally light proxy to simulations and to know how long an accurate simulation would take. We demonstrate that machine learning can build such surrogate and performance models with low errors for five previously developed models of HIV, while outperforming interpolation methods commonly used for surrogate modeling. © 2020 Society for Modeling & Simulation International (SCS)},
author_keywords={Cellular Automata;  HIV-1;  Interpolation;  Machine Learning;  Regression},
keywords={Decision making;  Diseases;  Forecasting;  Machine learning;  Technology transfer, Biological simulation;  Computational burden;  Computational resources;  Computing infrastructures;  Developed model;  Interpolation method;  Performance Model;  Surrogate model, Bioinformatics},
