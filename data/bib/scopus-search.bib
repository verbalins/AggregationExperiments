@CONFERENCE{Fang2019,
author={Fang, Z. and Lyu, J. and Jing, J. and Shao, H. and Li, X. and Cai, X.},
title={Research on real-time simulation modelling of Zhangbei renewable energy base based on RTDs},
journal={IET Conference Publications},
